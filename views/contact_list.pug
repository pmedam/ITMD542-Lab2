doctype html
html(lang="en")
  head
    meta(charset="UTF-8")
    meta(name="viewport", content="width=device-width, initial-scale=1.0")
    title Contact List
  body
    h2 Contact List
    table
      thead
        tr
          th Contact ID
          th First Name
          th Last Name
          th Email Address
          th Notes
          th Date/time created or last edited
      tbody
        each contact in contacts
          tr
            td= contact.contactID
            td= contact.firstName
            td= contact.lastName
            td= contact.emailAddress
            td= contact.notes
            td= contact.dateTime
